{
  "schema": "vvce.dsl.v1",
  "meta": {
    "id": "animation-demo",
    "version": "1.0.0",
    "title": "动画效果演示课程",
    "author": "VV Education",
    "description": "用于测试和验证各种动画效果的演示课程"
  },
  "globals": {
    "vars": {
      "score": 0,
      "attempt": 0,
      "currentAnimation": ""
    }
  },
  "resources": {
    "variables": {
      "colors": {
        "primary": "#4F46E5",
        "secondary": "#06B6D4",
        "success": "#10B981",
        "warning": "#F59E0B",
        "error": "#EF4444",
        "text": "#1F2937",
        "background": "#F9FAFB"
      },
      "spacing": {
        "xs": 4,
        "sm": 8,
        "md": 16,
        "lg": 24,
        "xl": 32
      },
      "fontSizes": {
        "sm": 14,
        "md": 16,
        "lg": 20,
        "xl": 24
      },
      "radii": {
        "sm": 4,
        "md": 8,
        "lg": 12,
        "full": 9999
      }
    },
    "animations": {
      "fadeInUp": {
        "keyframes": [
          { "offset": 0, "properties": { "opacity": 0, "translateY": 20 } },
          { "offset": 100, "properties": { "opacity": 1, "translateY": 0 } }
        ],
        "duration": 400,
        "easing": "ease-out"
      },
      "fadeInDown": {
        "keyframes": [
          { "offset": 0, "properties": { "opacity": 0, "translateY": -20 } },
          { "offset": 100, "properties": { "opacity": 1, "translateY": 0 } }
        ],
        "duration": 400,
        "easing": "ease-out"
      },
      "slideInLeft": {
        "keyframes": [
          { "offset": 0, "properties": { "opacity": 0, "translateX": -50 } },
          { "offset": 100, "properties": { "opacity": 1, "translateX": 0 } }
        ],
        "duration": 400,
        "easing": "ease-out"
      },
      "slideInRight": {
        "keyframes": [
          { "offset": 0, "properties": { "opacity": 0, "translateX": 50 } },
          { "offset": 100, "properties": { "opacity": 1, "translateX": 0 } }
        ],
        "duration": 400,
        "easing": "ease-out"
      },
      "bounceIn": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 0, "opacity": 0 } },
          { "offset": 60, "properties": { "scale": 1.15, "opacity": 1 } },
          { "offset": 80, "properties": { "scale": 0.9 } },
          { "offset": 100, "properties": { "scale": 1 } }
        ],
        "duration": 600,
        "easing": "ease-out"
      },
      "zoomIn": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 0.5, "opacity": 0 } },
          { "offset": 100, "properties": { "scale": 1, "opacity": 1 } }
        ],
        "duration": 400,
        "easing": "ease-out"
      },
      "pulse": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 1 } },
          { "offset": 50, "properties": { "scale": 1.08 } },
          { "offset": 100, "properties": { "scale": 1 } }
        ],
        "duration": 800,
        "iterations": 3,
        "easing": "ease-in-out"
      },
      "shake": {
        "keyframes": [
          { "offset": 0, "properties": { "translateX": 0 } },
          { "offset": 20, "properties": { "translateX": -10 } },
          { "offset": 40, "properties": { "translateX": 10 } },
          { "offset": 60, "properties": { "translateX": -10 } },
          { "offset": 80, "properties": { "translateX": 10 } },
          { "offset": 100, "properties": { "translateX": 0 } }
        ],
        "duration": 500,
        "easing": "ease-out"
      },
      "bounce": {
        "keyframes": [
          { "offset": 0, "properties": { "translateY": 0 } },
          { "offset": 40, "properties": { "translateY": -20 } },
          { "offset": 60, "properties": { "translateY": 0 } },
          { "offset": 80, "properties": { "translateY": -10 } },
          { "offset": 100, "properties": { "translateY": 0 } }
        ],
        "duration": 600,
        "easing": "ease-out"
      },
      "wobble": {
        "keyframes": [
          { "offset": 0, "properties": { "translateX": 0, "rotate": 0 } },
          { "offset": 15, "properties": { "translateX": -15, "rotate": -5 } },
          { "offset": 30, "properties": { "translateX": 10, "rotate": 3 } },
          { "offset": 45, "properties": { "translateX": -10, "rotate": -3 } },
          { "offset": 60, "properties": { "translateX": 5, "rotate": 2 } },
          { "offset": 75, "properties": { "translateX": -5, "rotate": -1 } },
          { "offset": 100, "properties": { "translateX": 0, "rotate": 0 } }
        ],
        "duration": 800,
        "easing": "ease-out"
      },
      "tada": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 1, "rotate": 0 } },
          { "offset": 10, "properties": { "scale": 0.9, "rotate": -3 } },
          { "offset": 20, "properties": { "scale": 0.9, "rotate": -3 } },
          { "offset": 30, "properties": { "scale": 1.1, "rotate": 3 } },
          { "offset": 40, "properties": { "scale": 1.1, "rotate": -3 } },
          { "offset": 50, "properties": { "scale": 1.1, "rotate": 3 } },
          { "offset": 60, "properties": { "scale": 1.1, "rotate": -3 } },
          { "offset": 70, "properties": { "scale": 1.1, "rotate": 3 } },
          { "offset": 80, "properties": { "scale": 1.1, "rotate": -3 } },
          { "offset": 90, "properties": { "scale": 1.1, "rotate": 3 } },
          { "offset": 100, "properties": { "scale": 1, "rotate": 0 } }
        ],
        "duration": 1000,
        "easing": "ease-out"
      },
      "heartbeat": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 1 } },
          { "offset": 14, "properties": { "scale": 1.3 } },
          { "offset": 28, "properties": { "scale": 1 } },
          { "offset": 42, "properties": { "scale": 1.3 } },
          { "offset": 70, "properties": { "scale": 1 } },
          { "offset": 100, "properties": { "scale": 1 } }
        ],
        "duration": 1200,
        "easing": "ease-in-out"
      },
      "celebrate": {
        "keyframes": [
          { "offset": 0, "properties": { "scale": 1, "rotate": 0 } },
          { "offset": 25, "properties": { "scale": 1.2, "rotate": -10 } },
          { "offset": 50, "properties": { "scale": 1.2, "rotate": 10 } },
          { "offset": 75, "properties": { "scale": 1.1, "rotate": -5 } },
          { "offset": 100, "properties": { "scale": 1, "rotate": 0 } }
        ],
        "duration": 800,
        "easing": "ease-out"
      },
      "flash": {
        "keyframes": [
          { "offset": 0, "properties": { "opacity": 1 } },
          { "offset": 25, "properties": { "opacity": 0 } },
          { "offset": 50, "properties": { "opacity": 1 } },
          { "offset": 75, "properties": { "opacity": 0 } },
          { "offset": 100, "properties": { "opacity": 1 } }
        ],
        "duration": 600,
        "easing": "linear"
      },
      "rubberBand": {
        "keyframes": [
          { "offset": 0, "properties": { "scaleX": 1, "scaleY": 1 } },
          { "offset": 30, "properties": { "scaleX": 1.25, "scaleY": 0.75 } },
          { "offset": 40, "properties": { "scaleX": 0.75, "scaleY": 1.25 } },
          { "offset": 50, "properties": { "scaleX": 1.15, "scaleY": 0.85 } },
          { "offset": 65, "properties": { "scaleX": 0.95, "scaleY": 1.05 } },
          { "offset": 75, "properties": { "scaleX": 1.05, "scaleY": 0.95 } },
          { "offset": 100, "properties": { "scaleX": 1, "scaleY": 1 } }
        ],
        "duration": 800,
        "easing": "ease-out"
      }
    },
    "transitions": {
      "fade": {
        "type": "fade",
        "duration": 300,
        "easing": "ease"
      },
      "slideLeft": {
        "type": "slide",
        "direction": "left",
        "duration": 400,
        "easing": "ease-in-out"
      },
      "slideRight": {
        "type": "slide",
        "direction": "right",
        "duration": 400,
        "easing": "ease-in-out"
      },
      "zoom": {
        "type": "zoom",
        "duration": 400,
        "easing": "ease-out"
      }
    },
    "styles": {
      "card": {
        "base": {
          "padding": 20,
          "borderRadius": 12,
          "backgroundColor": "#FFFFFF",
          "boxShadow": "0 4px 12px rgba(0,0,0,0.1)"
        }
      },
      "button-primary": {
        "base": {
          "padding": [12, 24],
          "borderRadius": 8,
          "backgroundColor": "#4F46E5",
          "color": "#FFFFFF",
          "fontSize": 16,
          "fontWeight": 600
        },
        "hover": {
          "backgroundColor": "#4338CA"
        }
      },
      "button-secondary": {
        "base": {
          "padding": [10, 20],
          "borderRadius": 8,
          "backgroundColor": "transparent",
          "borderWidth": 2,
          "borderColor": "#4F46E5",
          "color": "#4F46E5",
          "fontSize": 14
        }
      },
      "button-success": {
        "base": {
          "padding": [10, 20],
          "borderRadius": 8,
          "backgroundColor": "#10B981",
          "color": "#FFFFFF",
          "fontSize": 14
        }
      },
      "button-warning": {
        "base": {
          "padding": [10, 20],
          "borderRadius": 8,
          "backgroundColor": "#F59E0B",
          "color": "#FFFFFF",
          "fontSize": 14
        }
      },
      "button-error": {
        "base": {
          "padding": [10, 20],
          "borderRadius": 8,
          "backgroundColor": "#EF4444",
          "color": "#FFFFFF",
          "fontSize": 14
        }
      },
      "demo-target": {
        "base": {
          "padding": 40,
          "borderRadius": 16,
          "backgroundColor": "#4F46E5",
          "color": "#FFFFFF",
          "fontSize": 20,
          "fontWeight": 600,
          "textAlign": "center",
          "boxShadow": "0 8px 24px rgba(79, 70, 229, 0.3)"
        }
      }
    }
  },
  "theme": "playful",
  "startSceneId": "welcome",
  "scenes": [
    {
      "id": "welcome",
      "layout": {
        "type": "stack",
        "direction": "vertical",
        "gap": 20,
        "padding": 24,
        "align": "center"
      },
      "nodes": [
        {
          "id": "welcome-title",
          "type": "Dialog",
          "props": {
            "speaker": "VV老师",
            "text": "欢迎来到动画效果演示课程！在这里你可以测试各种动画效果。"
          },
          "style": {
            "animation": "fadeInDown"
          }
        },
        {
          "id": "intro-text",
          "type": "Dialog",
          "props": {
            "text": "点击下方按钮进入各个演示场景，每个场景都有对应的动画测试按钮。"
          },
          "style": {
            "animation": "fadeInUp",
            "animationDelay": 300
          }
        },
        {
          "id": "btn-entrance",
          "type": "Button",
          "props": {
            "text": "入场动画演示"
          },
          "styleClass": ["button-primary"],
          "style": {
            "animation": "bounceIn",
            "animationDelay": 500
          }
        },
        {
          "id": "btn-emphasis",
          "type": "Button",
          "props": {
            "text": "强调动画演示"
          },
          "styleClass": ["button-success"],
          "style": {
            "animation": "bounceIn",
            "animationDelay": 600
          }
        },
        {
          "id": "btn-quiz",
          "type": "Button",
          "props": {
            "text": "测验场景演示"
          },
          "styleClass": ["button-warning"],
          "style": {
            "animation": "bounceIn",
            "animationDelay": 700
          }
        }
      ],
      "triggers": [
        {
          "on": { "event": "click", "target": "btn-entrance" },
          "then": [
            { "action": "gotoScene", "sceneId": "entrance-demo" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-emphasis" },
          "then": [
            { "action": "gotoScene", "sceneId": "emphasis-demo" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-quiz" },
          "then": [
            { "action": "gotoScene", "sceneId": "quiz-demo" }
          ]
        }
      ],
      "transition": {
        "type": "fade",
        "duration": 300
      }
    },
    {
      "id": "entrance-demo",
      "layout": {
        "type": "stack",
        "direction": "vertical",
        "gap": 16,
        "padding": 24,
        "align": "center"
      },
      "nodes": [
        {
          "id": "entrance-title",
          "type": "Dialog",
          "props": {
            "speaker": "VV老师",
            "text": "入场动画测试 - 点击按钮查看不同的入场效果"
          },
          "style": {
            "animation": "fadeInDown"
          }
        },
        {
          "id": "demo-box",
          "type": "Dialog",
          "props": {
            "text": "动画演示区域"
          },
          "styleClass": ["demo-target"],
          "style": {
            "animation": "bounceIn"
          }
        },
        {
          "id": "btn-fadeInUp",
          "type": "Button",
          "props": { "text": "fadeInUp" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-fadeInDown",
          "type": "Button",
          "props": { "text": "fadeInDown" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-slideInLeft",
          "type": "Button",
          "props": { "text": "slideInLeft" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-slideInRight",
          "type": "Button",
          "props": { "text": "slideInRight" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-bounceIn",
          "type": "Button",
          "props": { "text": "bounceIn" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-zoomIn",
          "type": "Button",
          "props": { "text": "zoomIn" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-back-1",
          "type": "Button",
          "props": { "text": "返回主页" },
          "styleClass": ["button-error"]
        }
      ],
      "triggers": [
        {
          "on": { "event": "click", "target": "btn-fadeInUp" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "fadeInUp" },
            { "action": "toast", "text": "fadeInUp: 从下方淡入" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-fadeInDown" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "fadeInDown" },
            { "action": "toast", "text": "fadeInDown: 从上方淡入" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-slideInLeft" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "slideInLeft" },
            { "action": "toast", "text": "slideInLeft: 从左侧滑入" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-slideInRight" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "slideInRight" },
            { "action": "toast", "text": "slideInRight: 从右侧滑入" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-bounceIn" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "bounceIn" },
            { "action": "toast", "text": "bounceIn: 弹跳入场" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-zoomIn" },
          "then": [
            { "action": "playAnimation", "target": "demo-box", "animation": "zoomIn" },
            { "action": "toast", "text": "zoomIn: 缩放入场" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-back-1" },
          "then": [
            { "action": "gotoScene", "sceneId": "welcome" }
          ]
        }
      ],
      "transition": {
        "type": "slide",
        "direction": "left",
        "duration": 400
      }
    },
    {
      "id": "emphasis-demo",
      "layout": {
        "type": "stack",
        "direction": "vertical",
        "gap": 16,
        "padding": 24,
        "align": "center"
      },
      "nodes": [
        {
          "id": "emphasis-title",
          "type": "Dialog",
          "props": {
            "speaker": "VV老师",
            "text": "强调动画测试 - 这些动画用于吸引注意力或提供反馈"
          },
          "style": {
            "animation": "fadeInDown"
          }
        },
        {
          "id": "emphasis-box",
          "type": "Dialog",
          "props": {
            "text": "动画演示区域"
          },
          "styleClass": ["demo-target"],
          "style": {
            "animation": "bounceIn"
          }
        },
        {
          "id": "btn-pulse",
          "type": "Button",
          "props": { "text": "pulse (脉冲)" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-shake",
          "type": "Button",
          "props": { "text": "shake (抖动)" },
          "styleClass": ["button-error"]
        },
        {
          "id": "btn-bounce",
          "type": "Button",
          "props": { "text": "bounce (弹跳)" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-wobble",
          "type": "Button",
          "props": { "text": "wobble (摇摆)" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-tada",
          "type": "Button",
          "props": { "text": "tada (惊喜)" },
          "styleClass": ["button-success"]
        },
        {
          "id": "btn-heartbeat",
          "type": "Button",
          "props": { "text": "heartbeat (心跳)" },
          "styleClass": ["button-error"]
        },
        {
          "id": "btn-celebrate",
          "type": "Button",
          "props": { "text": "celebrate (庆祝)" },
          "styleClass": ["button-success"]
        },
        {
          "id": "btn-flash",
          "type": "Button",
          "props": { "text": "flash (闪烁)" },
          "styleClass": ["button-warning"]
        },
        {
          "id": "btn-rubberBand",
          "type": "Button",
          "props": { "text": "rubberBand (橡皮筋)" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-back-2",
          "type": "Button",
          "props": { "text": "返回主页" },
          "styleClass": ["button-error"]
        }
      ],
      "triggers": [
        {
          "on": { "event": "click", "target": "btn-pulse" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "pulse" },
            { "action": "toast", "text": "pulse: 用于提示可点击" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-shake" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "shake" },
            { "action": "toast", "text": "shake: 用于错误反馈" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-bounce" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "bounce" },
            { "action": "toast", "text": "bounce: 用于吸引注意" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-wobble" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "wobble" },
            { "action": "toast", "text": "wobble: 趣味强调效果" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-tada" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "tada" },
            { "action": "toast", "text": "tada: 惊喜奖励效果" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-heartbeat" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "heartbeat" },
            { "action": "toast", "text": "heartbeat: 重要提示" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-celebrate" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "celebrate" },
            { "action": "toast", "text": "celebrate: 答对庆祝" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-flash" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "flash" },
            { "action": "toast", "text": "flash: 闪烁警告" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-rubberBand" },
          "then": [
            { "action": "playAnimation", "target": "emphasis-box", "animation": "rubberBand" },
            { "action": "toast", "text": "rubberBand: 弹性效果" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-back-2" },
          "then": [
            { "action": "gotoScene", "sceneId": "welcome" }
          ]
        }
      ],
      "transition": {
        "type": "slide",
        "direction": "left",
        "duration": 400
      }
    },
    {
      "id": "quiz-demo",
      "layout": {
        "type": "stack",
        "direction": "vertical",
        "gap": 16,
        "padding": 24
      },
      "nodes": [
        {
          "id": "quiz-title",
          "type": "Dialog",
          "props": {
            "speaker": "VV老师",
            "text": "测验场景演示 - 体验答题时的动画反馈效果"
          },
          "style": {
            "animation": "fadeInDown"
          }
        },
        {
          "id": "quiz-question",
          "type": "QuizSingle",
          "props": {
            "question": "1 + 1 = ?",
            "options": ["1", "2", "3", "4"],
            "answerKey": "2"
          },
          "style": {
            "animation": "fadeInUp",
            "animationDelay": 200
          }
        },
        {
          "id": "submit-answer",
          "type": "Button",
          "props": { "text": "提交答案" },
          "styleClass": ["button-primary"],
          "style": {
            "animation": "bounceIn",
            "animationDelay": 400
          }
        },
        {
          "id": "score-display",
          "type": "Dialog",
          "props": {
            "text": "当前得分：{{globals.vars.score}} 分"
          },
          "style": {
            "animation": "fadeInUp",
            "animationDelay": 500
          }
        },
        {
          "id": "test-correct-anim",
          "type": "Button",
          "props": { "text": "测试正确动画 (celebrate)" },
          "styleClass": ["button-success"]
        },
        {
          "id": "test-wrong-anim",
          "type": "Button",
          "props": { "text": "测试错误动画 (shake)" },
          "styleClass": ["button-error"]
        },
        {
          "id": "test-sequence",
          "type": "Button",
          "props": { "text": "测试动画序列" },
          "styleClass": ["button-warning"]
        },
        {
          "id": "reset-score",
          "type": "Button",
          "props": { "text": "重置分数" },
          "styleClass": ["button-secondary"]
        },
        {
          "id": "btn-back-3",
          "type": "Button",
          "props": { "text": "返回主页" },
          "styleClass": ["button-error"]
        }
      ],
      "triggers": [
        {
          "on": { "event": "click", "target": "submit-answer" },
          "if": [
            {
              "op": "equals",
              "left": { "ref": "nodes.quiz-question.selected" },
              "right": "2"
            }
          ],
          "then": [
            { "action": "addScore", "value": 10 },
            { "action": "playAnimation", "target": "quiz-question", "animation": "celebrate" },
            { "action": "playAnimation", "target": "score-display", "animation": "bounceIn" },
            { "action": "toast", "text": "回答正确！+10分" }
          ],
          "else": [
            { "action": "incVar", "path": "globals.vars.attempt", "by": 1 },
            { "action": "playAnimation", "target": "quiz-question", "animation": "shake" },
            { "action": "toast", "text": "再想想～" }
          ]
        },
        {
          "on": { "event": "click", "target": "test-correct-anim" },
          "then": [
            { "action": "playAnimation", "target": "quiz-question", "animation": "celebrate" },
            { "action": "toast", "text": "celebrate 动画播放中" }
          ]
        },
        {
          "on": { "event": "click", "target": "test-wrong-anim" },
          "then": [
            { "action": "playAnimation", "target": "quiz-question", "animation": "shake" },
            { "action": "toast", "text": "shake 动画播放中" }
          ]
        },
        {
          "on": { "event": "click", "target": "test-sequence" },
          "then": [
            { "action": "playAnimation", "target": "quiz-question", "animation": "bounceIn" },
            { "action": "delay", "ms": 700 },
            { "action": "playAnimation", "target": "quiz-question", "animation": "pulse" },
            { "action": "delay", "ms": 900 },
            { "action": "playAnimation", "target": "quiz-question", "animation": "tada" },
            { "action": "delay", "ms": 1100 },
            { "action": "playAnimation", "target": "quiz-question", "animation": "celebrate" },
            { "action": "toast", "text": "动画序列: bounceIn → pulse → tada → celebrate" }
          ]
        },
        {
          "on": { "event": "click", "target": "reset-score" },
          "then": [
            { "action": "setVar", "path": "globals.vars.score", "value": 0 },
            { "action": "setVar", "path": "globals.vars.attempt", "value": 0 },
            { "action": "playAnimation", "target": "score-display", "animation": "flash" },
            { "action": "toast", "text": "分数已重置" }
          ]
        },
        {
          "on": { "event": "click", "target": "btn-back-3" },
          "then": [
            { "action": "gotoScene", "sceneId": "welcome" }
          ]
        }
      ],
      "transition": {
        "type": "slide",
        "direction": "left",
        "duration": 400
      }
    }
  ]
}
